{
  "metadata": [
    {
      "noRestore": true,
      "src": [
        {
          "src": "../../../Coree.NET",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../result/local",
    "template": [
      "statictoc",
      "../_exported_templates/override"
    ],
    "globalMetadata": {
      "_appName": "Coree.NET",
      "_appTitle": "Coree.NET",
      "_appFooter": "<pre></pre>"
    }
  }
}